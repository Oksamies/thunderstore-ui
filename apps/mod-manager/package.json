{
  "name": "@thunderstore/mod-manager",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:win": "tsc && vite build && electron-builder --win",
    "build:linux": "tsc && vite build && electron-builder --linux",
    "build:mac": "tsc && vite build && electron-builder --mac",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@node-steam/vdf": "^2.1.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@thunderstore/cyberstorm": "^0.1.0",
    "@thunderstore/dapper": "^0.1.0",
    "@thunderstore/dapper-ts": "^0.1.0",
    "@thunderstore/thunderstore-api": "^0.1.0",
    "@thunderstore/use-promise": "^0.1.0",
    "@types/react-window": "^2.0.0",
    "adm-zip": "^0.5.5",
    "ajv": "^8.17.1",
    "async-lock": "^1.4.1",
    "axios": "^0.24.0",
    "dexie": "^3.2.7",
    "electron-updater": "4.2.5",
    "fs-extra": "^8.1.0",
    "js-yaml": "^4.1.1",
    "moment": "^2.29.1",
    "path-browserify": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.4",
    "reselect": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/adm-zip": "^0.5.7",
    "@types/fs-extra": "^11.0.4",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.0",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^24.13.3",
    "typescript": "^5.6.2",
    "vite": "^7.2.6",
    "vite-plugin-electron": "^0.28.3",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "3.2.4"
  },
  "repository": "https://github.com/thunderstore-io/thunderstore-ui/tree/master/apps/mod-manager",
  "build": {
    "appId": "io.thunderstore.modmanager",
    "productName": "Thunderstore Mod Manager",
    "directories": {
      "output": "dist-electron/build"
    },
    "protocols": [
      {
        "name": "Thunderstore Mod Manager URL",
        "schemes": [
          "thunderstore"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Game"
    },
    "mac": {
      "category": "public.app-category.games"
    }
  }
}
