{
  "name": "@thunderstore/dapper",
  "version": "0.1.0",
  "description": "Data Provider using React Context for Thunderstore",
  "main": "dist/thunderstore-dapper.cjs.js",
  "module": "dist/thunderstore-dapper.esm.js",
  "exports": {
    ".": {
      "module": "./dist/thunderstore-dapper.esm.js",
      "default": "./dist/thunderstore-dapper.cjs.js"
    },
    "./schema": {
      "module": "./schema/dist/thunderstore-dapper-schema.esm.js",
      "default": "./schema/dist/thunderstore-dapper-schema.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "schema"
  ],
  "repository": "https://github.com/thunderstore-io/thunderstore-ui/tree/master/packages/dapper",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "@types/react": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {},
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "schema/index.ts"
    ],
    "exports": true
  }
}
